                function helpful(id){
                  $.ajax({
                    url: '/users/reviews',
                    method: 'patch',
                    data: {
                      reviewID : id
                    },
                    success: (res) => {
    if (res.success==1) {
      $('#helpful'+id).load(location.href + " #helpful"+id)
    }else{
      window.location.replace("/users/signIn")
    }
                    }
